<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AirFranceActivity">


    <ImageView
        android:id="@+id/airfrance"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:src="@drawable/airfrance"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <TextView
        android:id="@+id/airfrancetitle"
        style="@style/templatejudulmaskapai"
        android:text="@string/airfrancetitle"
        app:layout_constraintTop_toBottomOf="@id/airfrance"
        />

    <TextView
        android:id="@+id/airfrancenegara"
        style="@style/templatenegara"
        android:textColor="#FF000000"
        app:layout_constraintTop_toBottomOf="@+id/airfrancetitle"/>


    <TextView
        style="@style/templatenegara"
        android:text="@string/airfrancenegara"
        android:textColor="#FFFF6600"
        app:layout_constraintLeft_toRightOf="@id/airfrancenegara"
        app:layout_constraintTop_toBottomOf="@id/airfrancetitle"
        />

    <TextView
        android:id="@+id/airfrancehub"
        style="@style/templatehub"
        android:textColor="#FF000000"
        app:layout_constraintTop_toBottomOf="@+id/airfrancenegara"/>

    <TextView
        android:id="@+id/listhub"
        style="@style/templatehub"
        app:layout_constraintTop_toBottomOf="@id/airfrancehub"
        android:textColor="#FFFF6600"
        android:text="@string/airfrancehub"/>

    <TextView
        android:id="@+id/airfrancealiansi"
        style="@style/templatealiansi"
        android:textColor="#FF000000"
        app:layout_constraintTop_toBottomOf="@+id/listhub"/>

    <TextView
        android:id="@+id/aliansi"
        style="@style/templatealiansi"
        android:text="@string/airfrancealiansi"
        android:textColor="#FFFF6600"
        app:layout_constraintLeft_toRightOf="@id/airfrancealiansi"
        app:layout_constraintTop_toBottomOf="@id/listhub"
        />

    <TextView
        android:id="@+id/airfrancearmada"
        style="@style/templatearmada"
        android:textColor="#FF000000"
        app:layout_constraintTop_toBottomOf="@+id/airfrancealiansi"/>

    <TextView
        android:id="@+id/armada"
        style="@style/templatearmada"
        android:text="@string/airfrancearmada"
        android:textColor="#FFFF6600"
        app:layout_constraintLeft_toRightOf="@id/airfrancearmada"
        app:layout_constraintTop_toBottomOf="@id/airfrancealiansi"
        />

    <TextView
        android:id="@+id/airfrancetujuan"
        style="@style/templatetujuan"
        android:textColor="#FF000000"
        app:layout_constraintTop_toBottomOf="@+id/airfrancearmada"/>

    <TextView
        style="@style/templatetujuan"
        android:text="@string/airfrancetujuan"
        android:textColor="#FFFF6600"
        app:layout_constraintLeft_toRightOf="@id/airfrancetujuan"
        app:layout_constraintTop_toBottomOf="@id/airfrancearmada"
        />

</androidx.constraintlayout.widget.ConstraintLayout>